/***------ @BASE STYLES ------***/

html {
  
}
  body {
    font-family: "Lato", arial, sans-serif;
    
	background-image: url('../images/patterns/scribble_transparent.png');
	background-repeat: repeat;
	background-position: top right;
	
	-webkit-box-shadow: inset -20px -20px 50px rgba(0,0,0,0.1),inset 20px -20px 50px rgba(0,0,0,0.1);
	-moz-box-shadow: inset -20px -20px 50px rgba(0,0,0,0.1),inset 20px -20px 50px rgba(0,0,0,0.1);
	box-shadow: inset -20px -20px 50px rgba(0,0,0,0.1),inset 20px -20px 50px rgba(0,0,0,0.1);
  }

::-moz-selection { background: #0075cf; color: #fff; text-shadow: none; }
::selection { background: #0081c7; color: #fff; text-shadow: none; }

input::-webkit-input-placeholder {
      font-style: italic;
      color: #999 !important;
    }
input:focus::-webkit-input-placeholder{color:transparent;opacity: 0;
    filter:alpha(opacity=0);}
input::-moz-placeholder {
      font-style: italic;
      color: #999 !important;
    }
input:focus::-moz-placeholder{color:transparent;}

/***------ @STRUCTURE ------***/

.container-fluid{
	max-width: 980px;
	margin: 0 auto;
}
header{
	border-bottom: 3px solid #4d6f8a;
}
header p{
	color: #7e7e7e;
	font-size: 13px;
	letter-spacing: 1px;
	margin-left: 8px;
}
header h1{
	margin: 68px 0 10px;
	width: 100px;
}
header h1 a{
	display: block;
	font-size: 60px;
	background-size: 309px 42px;
	height: 42px;
	transition: all 0.3s linear;
	-o-transition: all 0.3s linear;
	-moz-transition: all 0.3s linear;
	-webkit-transition: all 0.3s linear;
}
header h1 a:hover{
	text-decoration: none;
}
h1{
	color: #444;
	font-size: 35px;
	margin: 33px 0 48px;
	max-width: 720px;
	line-height: 39px;
	font-weight: 600;
}
h2{
	color: #30629a;
	font-weight: 600;
}
p{
	color: #2b3c4e;
}
section{
	margin-bottom: 30px;
}
.item-row{
	margin: 40px 0;
}
.span4{
	transition: all 0.3s linear;
	-o-transition: all 0.3s linear;
	-moz-transition: all 0.3s linear;
	-webkit-transition: all 0.3s linear;
}

